generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Events {
  id          String      @id @default(uuid())
  name        String
  organizer   String
  imageUrl    String?
  startDate   DateTime?
  endDate     DateTime?
  description String?
}

model Projects {
  id           String      @id @default(uuid())
  projectName  String
  projectDoneBy String
  imageUrl     String?
  date         DateTime?
}

model Members {
  id               String      @id @default(uuid())
  name             String
  experience       String
  levelOfEducation String
  about            String?
}
